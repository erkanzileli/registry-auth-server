{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Go Run",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "args": ["runserver"],
      "env": {
        "REGISTRY_AUTH_CERT_PATH": "ssl/server.pem",
        "REGISTRY_AUTH_KEY_PATH": "ssl/server.key",
        "REGISTRY_AUTH_TOKEN_ISSUER": "auth.registry.ezileli.dev",
        "REGISTRY_AUTH_TOKEN_SERVICE": "registry.ezileli.dev",
        "AUTH_HOST": "0.0.0.0",
        "AUTH_PORT": "8000"
      }
    }
  ]
}
